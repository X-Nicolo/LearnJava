	public static void main(String[] args) {
		// TODO Auto-generated method stub
//		强制转换类型
		int x = 4;
		System.out.println(x);
		x = 10;
		System.out.println(x);
		
		/*
//		演示其他类型
		byte b = 2;//-128~127
		//byte b1 = 128;//错误：丢失精度；  
		short s = 30000;
		long l = 4l;//l标示 long类型  
		float f = 2.3f;//小数默认的double类型
		double d = 34.56;//双精度
		char ch = '4';//字符类型标示‘’//字符型
		boolean bo1 = false;
		*/
		
		

	}

}

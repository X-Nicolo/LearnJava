package day01;

public class day01_08 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
//		强制转换类型
		int x = 4;
		System.out.println(x);
		x = 10;
		System.out.println(x);
		
//		演示其他类型
		byte b = 2;//-128~127
		//byte b1 = 128;//错误：丢失精度；  
		short s = 30000;
		long l = 4l;l;l标示 long类型

	}

}
